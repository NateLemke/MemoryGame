<DOCTYPE html>
    <html>
        <head>
            <title>
                Memory Game
            </title>
            <link rel="stylesheet" type="text/css" href="css/leaderboardStyle.css">
            <script src="js/game.js"></script>
        </head>
        <body>
            <table id="Leaderboard">
                <tr>
                    <th>Name</th>
                    <th>Score</th>
                </tr>
                {{#each players}}
                <tr>
                    <td>{{this.name}}</td>
                    <td>{{this.score}}</td>
                </tr>
                {{/each}}
            </table>
        </body>
    </html>